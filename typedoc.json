{
    "$schema": "https://typedoc.org/schema.json",
    "entryPoints": [
        "./src/index.ts",
        "./src/components/index.ts",
        "./src/atom/index.ts",
        "./src/utils/index.ts",
    ],
    "out": "docs",
    "name": "SolarisUI",
    "includeVersion": true,
    "tsconfig": "./tsconfig.json",
   
    "visibilityFilters": {
        "protected": false,
        "private": false,
        "inherited": true,
        "external": false,
    },
    "sort": [
        "source-order",
        "visibility",
    ],
    "includes": "./static/",
    "plugin": ["typedoc-theme-yaf"],
    "theme": "yaf",
    "versions":{
        "dev":"v1.0.1-beta.0",
    }
    
}