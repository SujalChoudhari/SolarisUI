{
    "$schema": "https://typedoc.org/schema.json",
    "entryPoints": [
        "./src/index.ts",
        "./src/components/index.ts",
        "./src/atom/index.ts",
        "./src/utils/index.ts",
    ],
    "out": "docs",
    "name": "SolarisUI",
    "includeVersion": true,
    "tsconfig": "./tsconfig.json",
    "hideGenerator": true,
    "sidebarLinks": {
        "Github": "https://github.com/SujalChoudhari/SolarisUI",
        "LICENSE": "https://github.com/SujalChoudhari/SolarisUI/blob/main/LICENSE",
        "NPM":"https://www.npmjs.com/package/@sujalchoudhari/solaris-ui",
    },
    "visibilityFilters": {
        "protected": false,
        "private": false,
        "inherited": true,
        "external": false,
    },
    "sort": [
        "source-order",
        "visibility",
    ],
    "includes": "./static/",
    "plugin": ["typedoc-theme-yaf"],
    "versions":{
        "dev":"1.0.1-alpha.5"
        
    },
	"theme": "yaf",
    
}